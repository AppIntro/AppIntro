<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="-0.5dp"
        android:right="4.5dp"
        android:left="5dp">
        <shape android:shape="oval">
            <solid android:color="@color/appintro_default_unselected_color" />
            <size
                android:width="@dimen/appIntroIndicatorSize"
                android:height="@dimen/appIntroIndicatorSize" />
        </shape>
    </item>
    <item
        android:right="@dimen/appIntroIndicatorInset"
        android:left="@dimen/appIntroIndicatorInset">
        <shape android:shape="oval">
            <solid android:color="@color/appintro_default_selected_color" />
            <size
                android:width="@dimen/appIntroIndicatorSize"
                android:height="@dimen/appIntroIndicatorSize" />
        </shape>
    </item>
</layer-list>
